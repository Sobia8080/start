<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wsm.compose.compose_dao.dao.UserMapper">

    <select id="loginUser" resultType="com.wsm.compose.compose_domain.domain.SysUsers" parameterType="java.lang.String">
        select su.* from sys_users su where su.username=#{userName}
    </select>
</mapper>
